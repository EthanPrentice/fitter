<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FitButton">
        <attr name="buttonType">
            <enum name="primary" value="0" />
            <enum name="secondary" value="1" />
            <enum name="tertiary" value="2" />
        </attr>
        <attr name="text" format="string" />
        <attr name="buttonTextStyle" format="reference" />
        <attr name="icon" format="reference"/>
        <attr name="iconOn">
            <enum name="left"  value="0"/>
            <enum name="right" value="1"/>
        </attr>
        <attr name="android:clickable" format="boolean"/>
    </declare-styleable>
</resources>